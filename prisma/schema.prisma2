model Processo {
  idt_processo             Int               @id(clustered: false, map: "PK_Processo") @default(autoincrement())
  num_processo             Int?              @db.SmallInt
  ano_processo             Int?              @db.SmallInt
  cod_complemento          String?           @db.NVarChar(1)
  num_complemento          Int?              @db.TinyInt
  cod_complemento_old      String?           @db.NVarChar(1)
  des_assunto              String?           @db.NVarChar(80)
  idt_autor                Int?              @default(0, map: "DF_Processo_idt_autor")
  idt_interessado          Int?              @default(0, map: "DF_Processo_idt_interessado")
  idt_contratada           Int?              @default(0, map: "DF_Processo_idt_contratada")
  idt_modalidade           Int?              @default(0, map: "DF_Processo_idt_modalidade") @db.TinyInt
  dat_abertura             DateTime?         @db.SmallDateTime
  idt_pessoa_andamento     DateTime?         @default(dbgenerated("0"), map: "DF_Processo_idt_pessoa_andamento") @db.SmallDateTime
  dat_envio                DateTime?         @db.SmallDateTime
  des_local_arquivamento   String?           @db.NVarChar(20)
  idt_local_arquivo        Int?              @default(0, map: "DF_Processo_idt_local_arquivo")
  dat_arquivamento         DateTime?         @db.SmallDateTime
  flg_arquivamento         Boolean           @default(false, map: "DF_Processo_flg_arquivamento")
  idt_usuario_arquivamento Int?              @default(0, map: "DF_Processo_idt_usuario_arquivamento")
  idt_natureza             Int?              @default(0, map: "DF_Processo_idt_natureza")
  des_parecer              String?           @db.VarChar(1000)
  val_estimado             Float?            @default(0, map: "DF_Processo_val_estimado") @db.Real
  val_provisao             Float?            @default(0, map: "DF_Processo_val_provisao") @db.Real
  val_real                 Float?            @default(0, map: "DF_Processo_val_real") @db.Real
  idt_status               Int?              @default(0, map: "DF_Processo_idt_status")
  num_processo_str         String?           @unique(map: "idx_num_processo_str") @db.NVarChar(15)
  idt_fonte                Int?              @default(0, map: "DF_Processo_idt_fonte")
  idt_derivado             Int?              @default(0, map: "DF_Processo_idt_derivado")
  flg_provisao             Boolean           @default(false, map: "DF_Processo_flg_provisao")
  num_pag_docdigital       Int?              @default(0, map: "DF_Processo_num_pag_docdigital") @db.SmallInt
  idt_tipo_credor          Int?
  idt_credor_empresa       Int?
  idt_credor_pessoa        BigInt?
  idt_credor_colaborador   Int?
  idt_credor_outros        Int?
  idt_fonte_recurso        String?           @db.VarChar(30)
  idt_responsabilidade     Int?
  cod_cresp                String?           @db.Char(4)
  idt_credor_proc          Int?
  prcDocDigital            prcDocDigital[]
  Credor_Outros            Credor_Outros?    @relation(fields: [idt_credor_outros], references: [idt_credor], onDelete: NoAction, onUpdate: NoAction, map: "FK_Processo_Credor_Outros")
  Tipo_Credor              Tipo_Credor?      @relation(fields: [idt_tipo_credor], references: [idt_Tipo_Credor], onDelete: NoAction, onUpdate: NoAction, map: "FK_Processo_Tipo_Credor")
  ProcessoTramite          ProcessoTramite[]

  @@index([ano_processo], map: "idx_ano_processo")
  @@index([idt_credor_colaborador], map: "IDX_Credor_Colaborador")
  @@index([idt_credor_empresa], map: "IDX_Credor_Empresa")
  @@index([idt_credor_outros], map: "IDX_Credor_Outros")
  @@index([idt_credor_pessoa], map: "IDX_Credor_Pessoa")
  @@index([idt_fonte_recurso], map: "IDX_Fonte_Recurso")
  @@index([num_processo], map: "idx_num_processo")
  @@index([idt_contratada], map: "IX_Processo_01")
  @@index([idt_modalidade], map: "IX_Processo_02")
  @@index([idt_derivado], map: "IX_Processo_03")
  @@index([idt_natureza], map: "IX_Processo_04")
  @@index([idt_status], map: "IX_Processo_05")
  @@index([idt_fonte], map: "IX_Processo_06")

@@schema("BDControleProcesso].[dbo")
}

